<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 200 200">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4A90E2;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#87CEEB;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <circle cx="100" cy="100" r="98" fill="url(#bg)" />
  
  <!-- Clipboard -->
  <g transform="translate(50,40)">
    <!-- Clipboard Board -->
    <rect x="0" y="20" width="100" height="120" fill="#ECF0F1" rx="5" />
    <rect x="35" y="0" width="30" height="30" fill="#BDC3C7" rx="5" />
    
    <!-- Lines on Clipboard -->
    <line x1="20" y1="50" x2="80" y2="50" stroke="#95A5A6" stroke-width="3" />
    <line x1="20" y1="80" x2="80" y2="80" stroke="#95A5A6" stroke-width="3" />
    <line x1="20" y1="110" x2="80" y2="110" stroke="#95A5A6" stroke-width="3" />
  </g>
  
  <!-- Checkmark -->
  <g transform="translate(130,130) rotate(-45)">
    <path d="M0 0 L20 20 L50 -10" stroke="#27AE60" stroke-width="8" fill="none" stroke-linecap="round" stroke-linejoin="round" />
  </g>
</svg>